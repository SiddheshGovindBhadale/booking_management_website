<body onLoad="pending({{user._id}})">
<!-- massage -->
<div id="message"></div>
<!--loader-->
<div id="mains">
	    <div class="bg" >
		    <div class="loader"></div>
		</div>
	</div>

{{>userNav}}

<div class="container">
    <div class="main-body">
          <div class="row gutters-sm">
            <div class="col-md-4 mb-3">
              <div class="card">
                <div class="card-body">
                  <div class="d-flex flex-column align-items-center text-center">
                    <img src="./assets/img/icon/user.png" alt="User Profile" class="rounded-circle" width="150">
                    <div class="mt-3">
                      <h4>{{user.name}}</h4>
                      <!--<p class="text-secondary mb-1">Full Stack Developer</p>-->
                      <!--<p class="text-muted font-size-sm">Bay Area, San Francisco, CA</p>-->
                      <!--<button class="btn " style="background:#11406a; color:#fff;"  >Book more</button>-->
                      <!--<button class="btn" style="border:2px solid #11406a;"  >Message</button>-->
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div class="col-md-8">
              <div class="card mb-3">
                <div class="card-body">
                  <div class="row">
                    <div class="col-sm-3">
                      <h6 class="mb-0">Full Name</h6>
                    </div>
                    <div class="col-sm-9 text-secondary">
                      {{user.name}}
                    </div>
                  </div>
                  <hr>
                  <div class="row">
                    <div class="col-sm-3">
                      <h6 class="mb-0">Email</h6>
                    </div>
                    <div class="col-sm-9 text-secondary">
                      {{user.email}}
                    </div>
                  </div>
                  <hr>
                  <div class="row">
                    <div class="col-sm-3">
                      <h6 class="mb-0">Phone</h6>
                    </div>
                    <div class="col-sm-9 text-secondary">
                      {{user.phone}}
                    </div>
                  </div>
                  <hr>
                  <!--<hr>-->
                  <!--<div class="row">
                    <div class="col-sm-12">
                      <a class="btn btn-info " target="__blank" href="https://www.bootdey.com/snippets/view/profile-edit-data-and-skills">Edit</a>
                    </div>
                  </div>-->
                </div>
              </div>

            </div>
            
            <div class="buttons changeBtn" >
                 <button class="active" id="{{user._id}}" onclick="pending(this.id)">Pending</button>
                 <button id="{{user._id}}" onclick="collected(this.id)">Collected</button>
                 <button id="{{user._id}}" onclick="completed(this.id)">Completed</button>
            </div>
            <div class="table_responsive">
            <table>
            <thead>
            <tr>
            <th>No</th>
            <th>Date</th>
            <th>Slot</th>
            <th>Name</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Test</th>
            <th>Prescription</th>
            <th>Place</th>
            <th>Pincode</th>
            <th>Address</th>
            <th>status</th>
            <th>Action</th>
            </tr>
            </thead>
            
            <tbody>
            
            </tbody>
            </table>
            </div>
          </div>

        </div>
    </div>
    
<section id="models" >
         <!-- Prescription modal -->
    </section>
    
    {{>footer}}
    
    
    <!-- loader js -->
    <script src="./assets/js/loader.js"></script>
<!-- bootstrap js -->
