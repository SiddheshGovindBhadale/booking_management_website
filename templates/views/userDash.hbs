    <!-- coustom js -->
    <script src="./assets/js/main.js"></script>
    <!-- dashboard js -->
    <script src="./assets/js/dashboard.js"></script>
    <script>
        let d = {{user._id}}
        window.addEventListener('load', (event) =>{
        
        pending(d)
        
        });
    </script>
